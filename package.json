{
  "name": "ishtar",
  "version": "1.0.0",
  "description": "Dockerized Full-Stack EVE Online Discord Bot.",
  "license": "MIT",
  "repository": "https://github.com/kevinleung987/ishtar",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run build && node build/index.js",
    "build": "rm -rf build/ && tsc",
    "dev": "concurrently -k \"tsc -w\" \"nodemon --watch build ./build/index.js\"",
    "deploy": "heroku container:push web && heroku container:release web"
  },
  "dependencies": {
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/node": "^11.9.4",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3"
  }
}
